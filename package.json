{
  "name": "url-slug",
  "version": "3.0.6",
  "description": "Slug generator with less than 1 KB and no dependencies, RFC 3986 compliant",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup --config",
    "format": "npx prettier --write .",
    "lint": "npm run lint:eslint -- --ext cjs,cts,js,jsx,ts,tsx .",
    "lint:eslint": "eslint --cache --ignore-path .gitignore",
    "prepare": "husky install",
    "preversion": "npm test",
    "postversion": "git push && git push --tags",
    "test": "npm run lint && jest"
  },
  "devDependencies": {
    "@rollup/plugin-replace": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "cross-env": "^7.0.3",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "jest": "^27.5.1",
    "jest-esm-transformer": "^1.0.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "rollup": "^2.79.1",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^5.1.3"
  },
  "repository": "https://github.com/stldo/url-slug",
  "keywords": [
    "slug",
    "slugify",
    "url",
    "urlify",
    "url-safe",
    "rfc 3986",
    "string",
    "seo"
  ],
  "author": "stldo (https://github.com/stldo)",
  "license": "MIT"
}
